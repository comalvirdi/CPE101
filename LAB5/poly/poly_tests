import unittest
import from poly *

class TestCases(unittest.TestCase):
	def testPolyAdd(self):
		poly1 = [2.3, 4.7, 1.0]
		poly2 = [1.2, 2.1, -3.2]
		poly3 = poly_add2(poly1, poly2)
		self.assertListAlmostEqual(poly3, [3.5,6.8,-2.2])

if __name__ == '__main__':
   unittest.main()
